{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"],
			"env": ["PORT", "EXPO_PUBLIC_API_URL", "NEXT_PUBLIC_API_URL"]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"gen-env": {
			"dependsOn": []
		}
	}
}
