			"supportsTablet": true,
			"usesNonExemptEncryption": false
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.sahrohit.pracharya"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "859fb1a8-e41a-4330-abf6-70d5ed5675a2"
			}
		},
		"owner": "sahrohit",
		"plugins": ["expo-router"]
	}
}
